{
  "expo": {
    "name": "AIKO 2: Robotaxi",
    "slug": "aiko-2-robotaxi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aiko2robotaxi",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#faf8f5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.freeriverhouse.aiko2robotaxi"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#faf8f5"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.freeriverhouse.aiko2robotaxi"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "freeriverhouse"
  }
}
